{
  "version": 2,
  "buildCommand": "cd frontend-react && npm install && npm run build",
  "outputDirectory": "frontend-react/.next",
  "installCommand": "cd frontend-react && npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend-react/$1"
    }
  ]
}
